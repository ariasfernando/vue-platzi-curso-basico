    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "expect-more-jest": "^2.0.0",
    "front-matter": "^2.1.0",
    "jest": "^23.4.2",
    "jest-vue-preprocessor": "^1.4.0",
    "nock": "^9.3.3",
    "pre-commit": "^1.2.2",
    "pre-commit-with-lint": "^1.2.5",
    "stackup": "1.0.1",
    "stensul-coverage-metrics": "^1.0.7",
    "stylelint": "^9.6.0"
  },
  "pre-commit": {
    "lint": {
      "stylelint": ".(s?css|less)$",
      "eslint": ".(js|vue)$"
    },
    "run": [
      "cover"
    ]
  },
  "jest": {
    "setupFiles": [
      "./.jestrc.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "./node_modules/jest-vue-preprocessor",
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/assets/vue/$1",
      "customer(.*)$": "<rootDir>/stensul/customer/resources/assets/vue/$1"
    },
    "modulePaths": [
      "<rootDir>/node_modules",
      "<rootDir>/resources/assets"
    ],
    "roots": [
      "<rootDir>/tests/js",
      "<rootDir>/resources/assets/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!<rootDir>/.jestrc.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0.42,
        "functions": 0.39,
        "lines": 0.47,
        "statements": 0.47
      }
    },
    "testEnvironment": "node",
    "coverageDirectory": "./ci/storage/tmp/coverage",
    "verbose": true
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack --progress --config=./webpack.config.js",
    "watch": "NODE_ENV=development webpack --watch --progress --hide-modules --config=./webpack.config.js",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=./webpack.config.js",
    "watch:test": "npm t -- --watch",
    "cover": "npm t -- --coverage",
    "metricOfCover": "npx metricCover",
    "test": "jest",
    "eslint": "./node_modules/.bin/eslint ./resources/**/*.{js,vue} || echo \"\n$(tput setaf 1) CORRECT CODE IN THE ABOVE FILES AND TRY AGAIN $(tput sgr 0)\n\""
  },
  "metrics": {
    "coverUnitTest": {
      "pathReporter": "./ci/storage/tmp/coverage/clover.xml",
      "output": {
        "conexion": {
          "type": "mongoDb",
          "dbName": "coverage_reports",
          "collectionName": "coverages",
          "pathEnv": "./.env"
        }
      }
    }
  }
}
