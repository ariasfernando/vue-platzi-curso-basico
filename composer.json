{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.0",
		"jenssegers/mongodb": "2.1.7",
		"barryvdh/laravel-ide-helper": "~2.0",
		"Illuminate/html": "~5.0",
		"predis/predis": "~1.1@dev",
		"maxcdn/php-maxcdn": "2.*",
		"guzzlehttp/guzzle": "~6.0",
		"fideloper/proxy": "~3.0",
		"html2text/html2text": "^2.0",
		"aws/aws-sdk-php": "3.10.1",
		"league/flysystem-aws-s3-v3": "1.0.8",
		"laravel/socialite": "2.0.14",
		"folklore/image": "0.2.4",
		"exacttarget/fuel-sdk-php": "^0.9.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Stensul\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}

}
