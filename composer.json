{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "jenssegers/mongodb": "^3.2",
        "laravelcollective/html": "^5.3",
        "predis/predis": "^1.1",
        "aws/aws-sdk-php": "^3.19",
        "guzzlehttp/guzzle": "^6.2",
        "fideloper/proxy": "^3.1",
        "html2text/html2text": "^4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "laravel/socialite": "^2.0",
        "folklore/image": "^0.3.16",
        "exacttarget/fuel-sdk-php": "^0.9.0",
        "google/recaptcha": "^1.1",
        "graylog2/gelf-php": "^1.4",
        "stensul/php-imageproxy": "0.*",
        "olssonm/l5-zxcvbn": "^1.0",
        "hackzilla/password-generator": "^1.3",
        "akamai-open/edgegrid-client": "^1.0",
        "stensul/media-gallery": "dev-v2.1-SV2-264_media_gallery_integration"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "~2.0",
        "sabberworm/php-css-parser": "^8.1",
        "phpspec/phpspec": "~2.1"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://composer.stensuldev.net"
    }],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Stensul\\": "app/",
            "Stensul\\ImageProxy\\": "vendor/stensul/php-imageproxy/src/Stensul/ImageProxy"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
